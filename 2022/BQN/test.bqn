#!/usr/bin/env cbqn
⟨Split⟩ ← •Import "utils.bqn"
Ex ← {(@+10) Split 1⊑ •SH ⋈ •file.At 𝕩}
skip ← (¬⊑(<"all")∊•args)/⟨15,16,19⟩
Test ← { 𝕊 n‿x‿o:
  {
    ⊑n∊skip? •Out "Skipping day "∾•Repr n;
    ("day "∾•Repr n) ! o ≡ Ex x
  }
}
